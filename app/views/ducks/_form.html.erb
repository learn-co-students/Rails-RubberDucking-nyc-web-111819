
<% if flash[:messages] %>
  <h2>Errors</h2>
  <ul>
    <% flash[:messages].each do |error| %>
      <li><%= error %> </li>
    <% end%>
  </ul>
<% end%>


<p><%= form_for @duck do |f| %>
<p><%= f.label :name %>
<p><%= f.text_area :name %>
<p><%= f.label :description %>
<p><%= f.text_area :description %>
<p><%= f.label :student_id %>
<p><%= f.collection_select :student_id, @students, :id, :name, prompt: true %>
<p><%= f.submit %>
  
<% end %>