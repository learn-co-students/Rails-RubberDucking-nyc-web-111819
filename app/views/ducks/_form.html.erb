<% if flash[:messages] %>
    <h2>Errors:</h2>
    <ul>
    <% flash[:messages].each do |error| %>
        <li><%= error %></li>
        <% end %>
        </ul>
    <% end %>


<%= form_for @duck do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :description %>
    <%= f.text_field :description %><br>

    <%= f.label :student_id %>
    <%= f.collection_select :student_id, @students, :id, :name %><br>

    <%= f.submit %>
    <% end %>


    