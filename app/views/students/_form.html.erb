<% if flash[:messages] %>
    Errors
    <ul>
    <% flash[:messages].each do |error| %>
       <%= error %>
       </ul>
   <% end %>
   <% end %>

<%= form_for @student do |f| %>
   
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :mod %>
    <%= f.number_field :mod %>

    <%= f.submit %>
    <% end %>